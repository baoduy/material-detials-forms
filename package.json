{
  "name": "mui-formik-generator",
  "version": "1.0.0",
  "description": "Form and Details View generator using Material UI and Formik",
  "main": "index.tsx",
  "scripts": {
    "test": "jest --watch --coverage --config=configs/jest/ts.jest.json",
    "test-ci": "npm-run-all git:jest && jest --ci --coverage --config=configs/jest/ts.jest.json && codecov",
    "ci-publish": "ci-publish",
    "cost": "cost-of-modules --no-install",
    "update": "ncu -u && npm install",
    "update-g": "ncu -u -a -g",
    "start": "docz dev",
    "build-doc": "docz build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/baoduy/mui-formik-generator.git"
  },
  "keywords": [
    "Form",
    "Formik",
    "Material-UI",
    "Auto",
    "Generator"
  ],
  "author": "Duy Hoang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/baoduy/mui-formik-generator/issues"
  },
  "homepage": "https://github.com/baoduy/mui-formik-generator#readme",
  "dependencies": {
    "@babel/runtime": "^7.5.4",
    "@date-io/dayjs": "^1.3.8",
    "@elastic/numeral": "^2.3.3",
    "@material-ui/core": "^4.2.0",
    "@material-ui/pickers": "^3.1.2",
    "clsx": "^1.0.4",
    "dayjs": "^1.8.15",
    "formik": "^1.5.8",
    "linq": "^3.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-jss": "^10.0.0",
    "react-select": "^3.0.4",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/plugin-syntax-jsx": "^7.2.0",
    "@babel/plugin-transform-async-to-generator": "^7.5.0",
    "@babel/plugin-transform-modules-commonjs": "^7.5.0",
    "@babel/plugin-transform-react-display-name": "^7.2.0",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/plugin-transform-runtime": "^7.5.0",
    "@babel/preset-typescript": "^7.3.3",
    "@material-ui/icons": "^4.2.1",
    "@types/classnames": "^2.2.9",
    "@types/enzyme": "^3.10.2",
    "@types/jest": "^24.0.15",
    "@types/react": "^16.8.23",
    "@types/react-adal": "^0.4.1",
    "@types/react-dom": "^16.8.4",
    "@types/react-select": "^3.0.0",
    "@types/react-swipeable-views": "^0.13.0",
    "@types/react-table": "^6.8.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.12.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-transform-commonjs": "^1.1.5",
    "babel-plugin-transform-imports": "^2.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-preset-env": "^1.7.0",
    "babel-runtime": "^6.26.0",
    "docz": "^1.2.0",
    "docz-plugin-css": "^0.11.0",
    "docz-theme-default": "^1.2.0",
    "docz-theme-ztopia": "0.1.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "jest": "^24.8.0",
    "npm-run-all": "^4.1.5",
    "react-json-view": "^1.19.1",
    "ts-jest": "^24.0.2",
    "ts-loader": "^8.0.1",
    "typescript": "^3.5.3"
  },
  "jest": {
    "coverageDirectory": "<rootDir>/tests/__coverage__/",
    "collectCoverage": true
  },
  "bundlesize": [
    {
      "path": "./build/*.*",
      "compression": "gzip",
      "maxSize": "150 kB"
    },
    {
      "path": "./build/*.*",
      "compression": "none",
      "maxSize": "500 kB"
    }
  ]
}
